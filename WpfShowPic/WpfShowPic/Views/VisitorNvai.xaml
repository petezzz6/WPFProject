<UserControl x:Class="ShowPic.Views.VisitorNavi"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ShowPic.Views"
             mc:Ignorable="d" 
             d:DesignHeight="1000" d:DesignWidth="800">
    <Grid>
        <StackPanel Grid.Column="0" Grid.RowSpan="3" Background="CornflowerBlue">
            <Button Height="100" Content="图片列表" Command="{Binding JumpCommand}" 
                    CommandParameter="VisitorPictureView" Background="Transparent" BorderThickness="0" Margin="0 190 0 20 " FontSize="35" FontFamily="微软雅黑"></Button>
            <Button Height="100" Content="登录" Command="{Binding LoginCommand}" 
                     Background="Transparent" BorderThickness="0" FontSize="35" FontFamily="微软雅黑" Margin="0 0 0 20"></Button>
        </StackPanel>
    </Grid>
</UserControl>
